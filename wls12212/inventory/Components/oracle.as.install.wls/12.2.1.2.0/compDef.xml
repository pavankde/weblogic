<?xml version = '1.0' encoding = 'UTF-8' standalone = 'yes'?>
<component-def name="oracle.as.install.wls" version="12.2.1.2.0" description="Oracle Fusion Middleware WLS Installer Metadata Files" description-key="component.description" gdr-version="1.8.0.0" pom-gen="false" groupId="com.oracle.weblogic" xmlns:ns2="http://xmlns.oracle.com/cie/stringsubs" xmlns="http://xmlns.oracle.com/cie/gdr/cpd">
